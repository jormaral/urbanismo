<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <!-- CONFIGURACION WEB SERVICES-->
    <context-param>
        <param-name>wsdl-url</param-name>
        <param-value>http://localhost:8080/urbanismoenredWS/urbrWS?wsdl</param-value>
    </context-param>
    <!-- FIN CONFIGURACION WEB SERVICES-->
    <servlet>
        <servlet-name>ActionServlet</servlet-name>
        <servlet-class>DATA.ActionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletForWebServices</servlet-name>
        <servlet-class>DATA.ServletForWebServices</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletForBusquedas</servlet-name>
        <servlet-class>DATA.ServletForBusquedas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletForGenericServices</servlet-name>
        <servlet-class>DATA.ServletForGenericServices</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>readKML</servlet-name>
        <servlet-class>DATA.readKML</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>readGML</servlet-name>
        <servlet-class>DATA.readGML</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ActionServlet</servlet-name>
        <url-pattern>/ActionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletForWebServices</servlet-name>
        <url-pattern>/ServletForWebServices</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletForBusquedas</servlet-name>
        <url-pattern>/ServletForBusquedas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletForGenericServices</servlet-name>
        <url-pattern>/ServletForGenericServices</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>readKML</servlet-name>
        <url-pattern>/readKML</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>readGML</servlet-name>
        <url-pattern>/readGML</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
