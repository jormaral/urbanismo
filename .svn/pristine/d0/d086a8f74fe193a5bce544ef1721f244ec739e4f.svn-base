<!DOCTYPE composition PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
                             "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:s="http://jboss.com/products/seam/taglib"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:h="http://java.sun.com/jsf/html"
		xmlns:ice="http://www.icesoft.com/icefaces/component" 
                template="../layout/template.xhtml">
                       
<ui:define name="body">
    
    <ice:form id="operacionentidad" styleClass="edit">
     <ice:panelGroup  id="editoperacionentidadGroupId" styleClass="formBorderHighlight">
          <table width="100%" border="0" cellspacing="0" cellpadding="0">
              <tr>
                  <td class="iceDatTblColHdr2">
			    <ice:outputText id="editTextBoxId" value="#{operacionentidadHome.managed ? 'Edit' : 'Add'} operacionentidad"/>
                  </td>
              </tr>
          </table>

        <ice:panelGroup id="editPanelGroupId" styleClass="edit">		 


            <s:decorate id="idenField" template="../layout/edit.xhtml">
                <ui:define name="label">Iden</ui:define>
                        <ice:inputText id="iden"
			                partialSubmit="true"
                       required="true"
                       disabled="#{operacionentidadHome.managed}"
                                    value="#{operacionentidadHome.instance.iden}">
                        </ice:inputText>
            </s:decorate>




            <s:decorate id="idtipooperacionentField" template="../layout/edit.xhtml">
                <ui:define name="label">Idtipooperacionent</ui:define>
                        <ice:inputText id="idtipooperacionent"
			                partialSubmit="true"
                       required="true"
                                    value="#{operacionentidadHome.instance.idtipooperacionent}">
                        </ice:inputText>
            </s:decorate>

            <div style="clear:both">
                <span class="required">*</span> 
                required fields
            </div>
          </ice:panelGroup>
       </ice:panelGroup>
                  
       <div class="actionButtons">
        
            <ice:commandButton id="save" 
                          value="Save" 
                         action="#{operacionentidadHome.persist}"
                       disabled="#{!operacionentidadHome.wired}"
                       rendered="#{!operacionentidadHome.managed}"/>  
                          			  
            <ice:commandButton id="update" 
                          value="Save" 
                         action="#{operacionentidadHome.update}"
                       rendered="#{operacionentidadHome.managed}"/>
                        			  
            <ice:commandButton id="delete" 
                          value="Delete" 
                         action="#{operacionentidadHome.remove}"
                      immediate="true"
                       rendered="#{operacionentidadHome.managed}"/>
                    
            <s:button id="cancelEdit" styleClass="iceCmdBtn"
                   value="Cancel"
             propagation="end"
                    view="/seam/Operacionentidad.xhtml"
                rendered="#{operacionentidadHome.managed}"/>
                
            <s:button id="cancelAddoperacionentidadHome" styleClass="iceCmdBtn"
                   value="Cancel"
             propagation="end"
                    view="/seam/#{empty operacionentidadFrom ? 'OperacionentidadList' : operacionentidadFrom}.xhtml"
                rendered="#{!operacionentidadHome.managed}"/>
                
        </div>
        
    </ice:form>

 <ice:form id="form2operacionentidadHome">  
        <ice:panelTabSet id="editPanelTaboperacionentidadHomeId" styleClass="componentPanelTabSetLayout" style="margin-bottom:5px;margin-top:10px;">
   
 	<ice:panelTab id="editTabentidadByIdentidadId" label="Entidad by identidad *" >
		<div class="association" id="entidadByIdentidadParent">
    
        	<h:outputText id="editentidadByIdentidadTextId" value="There is no entidadByIdentidad associated with this operacionentidad." 
                   rendered="#{operacionentidadHome.instance.entidadByIdentidad == null}"/>
       	<ice:dataTable var="entidad" 
                   value="#{operacionentidadHome.instance.entidadByIdentidad}" 
                rendered="#{operacionentidadHome.instance.entidadByIdentidad != null}"
              rowClasses="rvgRowOne,rvgRowTwo"
		  columnClasses="allCols"
                      id="editentidadByIdentidadTableId">
            <ice:column id="$editColumnidenId">
                <f:facet name="header">Iden</f:facet>
                #{entidad.iden}
            </ice:column>
            <ice:column id="$editColumnbsuspendidaId">
                <f:facet name="header">Bsuspendida</f:facet>
                #{entidad.bsuspendida}
            </ice:column>
            <ice:column id="$editColumnclaveId">
                <f:facet name="header">Clave</f:facet>
                #{entidad.clave}
            </ice:column>
            <ice:column id="$editColumncodigoId">
                <f:facet name="header">Codigo</f:facet>
                #{entidad.codigo}
            </ice:column>
            <ice:column id="$editColumnentidadByIdentidadbaseId">
	    	    <f:facet name="header">Entidad by identidadbase iden</f:facet>
		    	#{entidad.entidadByIdentidadbase.iden}
            </ice:column>
            <ice:column id="$editColumnentidadByIdentidadoriginalId">
	    	    <f:facet name="header">Entidad by identidadoriginal iden</f:facet>
		    	#{entidad.entidadByIdentidadoriginal.iden}
            </ice:column>
            <ice:column id="$editColumnentidadByIdpadreId">
	    	    <f:facet name="header">Entidad by idpadre iden</f:facet>
		    	#{entidad.entidadByIdpadre.iden}
            </ice:column>
            <ice:column id="$editColumnetiquetaId">
                <f:facet name="header">Etiqueta</f:facet>
                #{entidad.etiqueta}
            </ice:column>
            <ice:column id="$editColumnnombreId">
                <f:facet name="header">Nombre</f:facet>
                #{entidad.nombre}
            </ice:column>
            <ice:column id="$editColumnordenId">
                <f:facet name="header">Orden</f:facet>
                #{entidad.orden}
            </ice:column>
            <ice:column id="$editColumntramiteId">
	    	    <f:facet name="header">Tramite iden</f:facet>
		    	#{entidad.tramite.iden}
            </ice:column>
            <ice:column id="editColumnentidadLinkId">
                <f:facet name="header">Action</f:facet>
                <s:link view="/seam/Entidad.xhtml" 
                         id="viewentidad" 
                      value="View" 
                propagation="none">
                    <f:param name="entidadIden" 
                           value="#{entidad.iden}"/>
                </s:link>
            </ice:column>
      </ice:dataTable>
        <div class="actionButtons">
            <s:button value="Select entidadByIdentidad"
	              id="selectParent"
                      view="/seam/EntidadList.xhtml">
                <f:param name="from" value="OperacionentidadEdit"/>
            </s:button>
        </div>
        
    </div>
    </ice:panelTab>
   
 	<ice:panelTab id="editTabentidadByIdentidadoperadoraId" label="Entidad by identidadoperadora *" >
		<div class="association" id="entidadByIdentidadoperadoraParent">
    
        	<h:outputText id="editentidadByIdentidadoperadoraTextId" value="There is no entidadByIdentidadoperadora associated with this operacionentidad." 
                   rendered="#{operacionentidadHome.instance.entidadByIdentidadoperadora == null}"/>
       	<ice:dataTable var="entidad" 
                   value="#{operacionentidadHome.instance.entidadByIdentidadoperadora}" 
                rendered="#{operacionentidadHome.instance.entidadByIdentidadoperadora != null}"
              rowClasses="rvgRowOne,rvgRowTwo"
		  columnClasses="allCols"
                      id="editentidadByIdentidadoperadoraTableId">
            <ice:column id="$editColumnidenId">
                <f:facet name="header">Iden</f:facet>
                #{entidad.iden}
            </ice:column>
            <ice:column id="$editColumnbsuspendidaId">
                <f:facet name="header">Bsuspendida</f:facet>
                #{entidad.bsuspendida}
            </ice:column>
            <ice:column id="$editColumnclaveId">
                <f:facet name="header">Clave</f:facet>
                #{entidad.clave}
            </ice:column>
            <ice:column id="$editColumncodigoId">
                <f:facet name="header">Codigo</f:facet>
                #{entidad.codigo}
            </ice:column>
            <ice:column id="$editColumnentidadByIdentidadbaseId">
	    	    <f:facet name="header">Entidad by identidadbase iden</f:facet>
		    	#{entidad.entidadByIdentidadbase.iden}
            </ice:column>
            <ice:column id="$editColumnentidadByIdentidadoriginalId">
	    	    <f:facet name="header">Entidad by identidadoriginal iden</f:facet>
		    	#{entidad.entidadByIdentidadoriginal.iden}
            </ice:column>
            <ice:column id="$editColumnentidadByIdpadreId">
	    	    <f:facet name="header">Entidad by idpadre iden</f:facet>
		    	#{entidad.entidadByIdpadre.iden}
            </ice:column>
            <ice:column id="$editColumnetiquetaId">
                <f:facet name="header">Etiqueta</f:facet>
                #{entidad.etiqueta}
            </ice:column>
            <ice:column id="$editColumnnombreId">
                <f:facet name="header">Nombre</f:facet>
                #{entidad.nombre}
            </ice:column>
            <ice:column id="$editColumnordenId">
                <f:facet name="header">Orden</f:facet>
                #{entidad.orden}
            </ice:column>
            <ice:column id="$editColumntramiteId">
	    	    <f:facet name="header">Tramite iden</f:facet>
		    	#{entidad.tramite.iden}
            </ice:column>
            <ice:column id="editColumnentidadLinkId">
                <f:facet name="header">Action</f:facet>
                <s:link view="/seam/Entidad.xhtml" 
                         id="viewentidad" 
                      value="View" 
                propagation="none">
                    <f:param name="entidadIden" 
                           value="#{entidad.iden}"/>
                </s:link>
            </ice:column>
      </ice:dataTable>
        <div class="actionButtons">
            <s:button value="Select entidadByIdentidadoperadora"
	              id="selectParent"
                      view="/seam/EntidadList.xhtml">
                <f:param name="from" value="OperacionentidadEdit"/>
            </s:button>
        </div>
        
    </div>
    </ice:panelTab>
   
 	<ice:panelTab id="editTaboperacionId" label="Operacion *" >
		<div class="association" id="operacionParent">
    
        	<h:outputText id="editoperacionTextId" value="There is no operacion associated with this operacionentidad." 
                   rendered="#{operacionentidadHome.instance.operacion == null}"/>
       	<ice:dataTable var="operacion" 
                   value="#{operacionentidadHome.instance.operacion}" 
                rendered="#{operacionentidadHome.instance.operacion != null}"
              rowClasses="rvgRowOne,rvgRowTwo"
		  columnClasses="allCols"
                      id="editoperacionTableId">
            <ice:column id="$editColumnidenId">
                <f:facet name="header">Iden</f:facet>
                #{operacion.iden}
            </ice:column>
            <ice:column id="$editColumnordenId">
                <f:facet name="header">Orden</f:facet>
                #{operacion.orden}
            </ice:column>
            <ice:column id="$editColumntextoId">
                <f:facet name="header">Texto</f:facet>
                #{operacion.texto}
            </ice:column>
            <ice:column id="$editColumntramiteId">
	    	    <f:facet name="header">Tramite iden</f:facet>
		    	#{operacion.tramite.iden}
            </ice:column>
            <ice:column id="editColumnoperacionLinkId">
                <f:facet name="header">Action</f:facet>
                <s:link view="/seam/Operacion.xhtml" 
                         id="viewoperacion" 
                      value="View" 
                propagation="none">
                    <f:param name="operacionIden" 
                           value="#{operacion.iden}"/>
                </s:link>
            </ice:column>
      </ice:dataTable>
        <div class="actionButtons">
            <s:button value="Select operacion"
	              id="selectParent"
                      view="/seam/OperacionList.xhtml">
                <f:param name="from" value="OperacionentidadEdit"/>
            </s:button>
        </div>
        
    </div>
    </ice:panelTab>
</ice:panelTabSet>
</ice:form>
</ui:define>

</ui:composition>



