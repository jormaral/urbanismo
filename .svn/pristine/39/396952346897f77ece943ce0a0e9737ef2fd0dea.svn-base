<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:ice="http://www.icesoft.com/icefaces/component">
 
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:s="http://jboss.com/products/seam/taglib"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ice="http://www.icesoft.com/icefaces/component"
	template="../layout/templateArbol.xhtml">

 

	<ui:define name="body">

		<ice:form>
			<ice:panelGrid columns="2" columnClasses="testCol1,testCol2">
				<ice:panelGroup>
				

					<ice:panelTabSet id="panelTabArbolEnt" styleClass="arbol">
						<ice:panelTab id="tabArbolEnt" label="#{messages.arbol_determinaciones}">
							<!-- TRee values -->
							<ice:panelGroup styleClass="arbol" scrollHeight="800px"
								scrollWidth="40em">
								<ice:tree id="tree" value="#{gestionArbolDeterminaciones.model}"
									var="node" binding="#{gestionArbolDeterminaciones.treeComponent}"
									imageDir="/fipeditor/xmlhttp/css/rime/css-images/"
									hideRootNode="false" hideNavigation="false"
									actionListener="#{gestionArbolDeterminaciones.menuEvent}">
									<ice:treeNode>


										<f:facet name="icon">
											<ice:panelGroup style="display: inline">
												<h:graphicImage
													value="/xmlhttp/css/rime/css-images/#{node.userObject.icon}" />

											</ice:panelGroup>
										</f:facet>
										<f:facet name="content">
											<ice:panelGroup
												styleClass="condUrb#{node.userObject.datosIdTextoArbolGenerico.tipo == 'sicu'}"
												style="display: inline">
												<ice:panelGroup
													styleClass="selectedNode#{node.userObject eq gestionArbolDeterminaciones.selectedUserObject}"
													style="display: inline">

													<ice:commandLink
														actionListener="#{gestionArbolDeterminaciones.nodeSelected}"
														disabled="#{node.userObject.datosIdTextoArbolGenerico.tipo == 'nocu'}">
														<f:param name="nodoId"
															value="#{node.userObject.datosIdTextoArbolGenerico.idBaseDatos}" />
														<ice:outputText id="TreeNode"
															value="#{node.userObject.text}" />
													</ice:commandLink>
												</ice:panelGroup>
											</ice:panelGroup>
										</f:facet>

									</ice:treeNode>
								</ice:tree>


							</ice:panelGroup>
						</ice:panelTab>
					</ice:panelTabSet>


				
				</ice:panelGroup>

				<ice:panelGroup
					styleClass="exampleBox firstChildTitle backBeanChangeContainer">
	
					<!-- backing bean title box -->
					<ice:panelGroup styleClass="backBeanChangeTitle"
						style="width:600px;">
						<ice:outputText value="#{messages.valores}" />
					</ice:panelGroup>
	
					<!-- actual backing bean values. -->
					<ice:panelGroup styleClass="backBeanChangeBody"
						effect="#{gestionArbolDeterminaciones.valueChangeEffect}">
						<ice:panelGroup style="margin: 0; padding: 0;width:600px;">
							<ice:panelGrid cellpadding="0" cellspacing="3" columns="2">
	
								<ice:panelGroup	styleClass="exampleBox firstChildTitle backBeanChangeContainer">
								<ice:panelSeries id="icePnlSrs"
									value="#{gestionArbolDeterminaciones.selectedTreePath}" var="treePath">
									<ice:graphicImage width="11" height="8"
										style="margin-left:3px;margin-right:3px;"
										value="/xmlhttp/css/rime/css-images/bullet.gif" />
									<ice:outputText id="treePath" value="#{treePath}" />
								</ice:panelSeries>
								</ice:panelGroup>
								
								<ice:panelGroup styleClass="labels" >
								<ice:outputLabel value=" " />
								</ice:panelGroup>
								<ice:panelGroup	styleClass="exampleBoxLabelName">
								<ice:outputText id="employeeName" styleClass="nameFont"
									value="#{gestionArbolDeterminaciones.selectedUserObject.datosIdTextoArbolGenerico.texto}" />
								</ice:panelGroup>
							</ice:panelGrid>
						</ice:panelGroup>
						
								<ice:panelGrid columns="2"
									columnClasses="testColLabel,testColGeom" width="300px">
	
									<ice:panelGroup style="margin: 0; padding: 0; ">
										<ice:panelGrid cellpadding="0" cellspacing="3" columns="2"
											columnClasses="colLabel,colInput">
	
											<ice:panelGroup styleClass="labels">
												<ice:outputLabel value="#{messages.determinacion_base}" />
											</ice:panelGroup>
											<ice:panelGroup styleClass="exampleBoxLabel">
												<ice:outputText id="employeeBaseDet"
													value="#{determinacionHome.instance.getDeterminacionByIddeterminacionbase().nombre}" />
											</ice:panelGroup>
	
											<ice:panelGroup styleClass="labels">
												<ice:outputLabel value="#{messages.determinacion_tipo}" />
											</ice:panelGroup>
											<ice:panelGroup styleClass="exampleBoxLabel">
												<ice:outputText id="employeeKey"
													value="#{determinacionHome.instance.iden}" />
											</ice:panelGroup>
	
											<ice:panelGroup styleClass="labels">
												<ice:outputLabel value="#{messages.determinacion_articulado}" />
											</ice:panelGroup>
											<ice:panelGroup styleClass="exampleBoxLabel">
												<ice:outputText value="#{determinacionHome.instance.iden}"/>
											</ice:panelGroup>
											
											<ice:panelGroup styleClass="labels">
												<ice:outputLabel value="#{messages.determinacion_unidad}" />
											</ice:panelGroup>
											<ice:panelGroup styleClass="exampleBoxLabel">
												<ice:outputText value="#{determinacionHome.instance.iden}"/>
											</ice:panelGroup>
											
											<ice:panelGroup styleClass="labels">
												<ice:outputLabel value="#{messages.determinacion_grupoaplicacion}" />
											</ice:panelGroup>
											<ice:panelGroup styleClass="exampleBoxLabel">
												<ice:outputText value="#{determinacionHome.instance.iden}"/>
											</ice:panelGroup>
											
											<ice:panelGroup styleClass="labels">
												<ice:outputLabel value="#{messages.determinacion_reguladora}" />
											</ice:panelGroup>
											<ice:panelGroup styleClass="exampleBoxLabel">
												<ice:outputText value="#{determinacionHome.instance.iden}"/>
											</ice:panelGroup>
	
											<ice:panelGroup styleClass="labels">
												<ice:outputLabel value="#{messages.determinacion_simbologia}" />
											</ice:panelGroup>
											<ice:panelGroup styleClass="exampleBoxLabel">
												<ice:outputText value="#{determinacionHome.instance.iden}"/>
											</ice:panelGroup>
											
											<ice:panelGroup styleClass="labels">
												<ice:outputLabel value="#{messages.determinacion_definicion}" />
											</ice:panelGroup>
											<ice:panelGroup styleClass="exampleBoxLabel">
												<ice:outputText value="#{determinacionHome.instance.iden}"/>
											</ice:panelGroup>
	
										</ice:panelGrid>
									</ice:panelGroup>
	
									<ice:panelGroup>
										<ice:outputLabel value="#{messages.determinacion_valoresreferencia}" styleclass="geom" />
										
									</ice:panelGroup>
								</ice:panelGrid>
							</ice:panelGroup>
						</ice:panelGroup>
						
				
					
			
			</ice:panelGrid>
		</ice:form>
	</ui:define>
</ui:composition>
</html>
