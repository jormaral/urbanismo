<!DOCTYPE composition PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
                             "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">


<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:s="http://jboss.com/products/seam/taglib"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:h="http://java.sun.com/jsf/html"
		xmlns:ice="http://www.icesoft.com/icefaces/component"  
                template="../layout/template.xhtml">
                       
<ui:define name="body">
    
    <ice:form id="operacionentidadSearch" styleClass="edit">
      <ice:panelGroup  id="searchGroup" styleClass="formBorderHighlight">
          <table width="100%" border="0" cellspacing="0" cellpadding="0">
              <tr>
                  <td class="iceDatTblColHdr2">
                    <ice:outputText id="listOperacionentidadId" value="Operacionentidad search filter"/>
                 </td>
              </tr>
         </table>	
         <ice:panelGroup id="listPanelGroupOperacionentidadId" styleClass="edit">
		
            <s:decorate template="../layout/display.xhtml">
                <ui:define name="label">Match</ui:define>
                <ice:selectOneRadio id="logic" value="#{operacionentidadList.restrictionLogicOperator}" partialSubmit="true">
                    <f:selectItem itemLabel="All" itemValue="and"/>
                    <f:selectItem itemLabel="Any" itemValue="or"/>
                </ice:selectOneRadio>
            </s:decorate>
          
   
	  </ice:panelGroup>
  
        
        <div class="actionButtons">
            <ice:commandButton id="search" value="Search" action="/OperacionentidadList.xhtml"/>
        </div>
      </ice:panelGroup> 
    </ice:form>
    
    <ice:panelGroup styleClass="formBorderHighlight">

    <h3>operacionentidad  search results</h3>

    <div class="searchResults" id="listoperacionentidadResults">
    <ice:outputText value="The operacionentidad search returned no results." 
               rendered="#{empty operacionentidadList.resultList}"/>
               
    <ice:dataTable id="operacionentidadList" 
                  var="operacionentidad"
                value="#{operacionentidadList.resultList}" 
            resizable="true"
	columnClasses="allCols"
             rendered="#{not empty operacionentidadList.resultList}">
        <ice:column id="listidenId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{operacionentidadList}"/>
                    <ui:param name="propertyLabel" value="Iden"/>
                    <ui:param name="propertyPath" value="operacionentidad.iden"/>
                </ui:include>
            </f:facet>
            #{operacionentidad.iden}&amp;nbsp;
        </ice:column>
        <ice:column id="listColumnentidadByIdentidadId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{operacionentidadList}"/>
                    <ui:param name="propertyLabel" value="Entidad by identidad iden"/>
                    <ui:param name="propertyPath" value="operacionentidad.entidadByIdentidad.iden"/>
                </ui:include>
            </f:facet>
            #{operacionentidad.entidadByIdentidad.iden}
        </ice:column>
        <ice:column id="listColumnentidadByIdentidadoperadoraId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{operacionentidadList}"/>
                    <ui:param name="propertyLabel" value="Entidad by identidadoperadora iden"/>
                    <ui:param name="propertyPath" value="operacionentidad.entidadByIdentidadoperadora.iden"/>
                </ui:include>
            </f:facet>
            #{operacionentidad.entidadByIdentidadoperadora.iden}
        </ice:column>
        <ice:column id="listidtipooperacionentId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{operacionentidadList}"/>
                    <ui:param name="propertyLabel" value="Idtipooperacionent"/>
                    <ui:param name="propertyPath" value="operacionentidad.idtipooperacionent"/>
                </ui:include>
            </f:facet>
            #{operacionentidad.idtipooperacionent}&amp;nbsp;
        </ice:column>
        <ice:column id="listColumnoperacionId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{operacionentidadList}"/>
                    <ui:param name="propertyLabel" value="Operacion iden"/>
                    <ui:param name="propertyPath" value="operacionentidad.operacion.iden"/>
                </ui:include>
            </f:facet>
            #{operacionentidad.operacion.iden}
        </ice:column>
        <ice:column id="listColumnOperacionentidadId">
            <f:facet name="header">Action</f:facet>
            <s:link view="/seam/#{empty from ? 'Operacionentidad' : from}.xhtml" 
                   value="#{empty from ? 'View' : 'Select'}" 
                      id="operacionentidad">
                <f:param name="operacionentidadIden" 
                        value="#{operacionentidad.iden}"/>
            </s:link>
			#{' '}
            <s:link view="/seam/OperacionentidadEdit.xhtml"
                   value="Edit" 
                      id="operacionentidadEdit"
                      rendered="#{empty from}">
                <f:param name="operacionentidadIden" 
                        value="#{operacionentidad.iden}"/>
            </s:link>
        </ice:column>
    </ice:dataTable>

    </div>
</ice:panelGroup>

    <div class="tableControl">
      
        <s:link view="/seam/OperacionentidadList.xhtml" 
            rendered="#{operacionentidadList.previousExists}" 
               value="#{messages.left}#{messages.left} First Page"
                  id="firstPageoperacionentidadListId">
          <f:param name="firstResult" value="0"/>
        </s:link>
        
        <s:link view="/seam/OperacionentidadList.xhtml" 
            rendered="#{operacionentidadList.previousExists}" 
               value="#{messages.left} Previous Page"
                  id="previousPageoperacionentidadListId">
            <f:param name="firstResult" 
                    value="#{operacionentidadList.previousFirstResult}"/>
        </s:link>
        
        <s:link view="/seam/OperacionentidadList.xhtml" 
            rendered="#{operacionentidadList.nextExists}" 
               value="Next Page #{messages.right}"
                  id="nextPageoperacionentidadListId">
            <f:param name="firstResult" 
                    value="#{operacionentidadList.nextFirstResult}"/>
        </s:link>
        
        <s:link view="/seam/OperacionentidadList.xhtml" 
            rendered="#{operacionentidadList.nextExists}" 
               value="Last Page #{messages.right}#{messages.right}"
                  id="lastPageoperacionentidadListId">
            <f:param name="firstResult" 
                    value="#{operacionentidadList.lastFirstResult}"/>
        </s:link>
        
    </div>
    
    <s:div styleClass="actionButtons" rendered="#{empty from}">
        <s:button view="/seam/OperacionentidadEdit.xhtml"
                    id="create" 
                 value="Create operacionentidad">
            <f:param name="operacionentidadIden"/>
        </s:button>
    </s:div>
    
</ui:define>

</ui:composition>

