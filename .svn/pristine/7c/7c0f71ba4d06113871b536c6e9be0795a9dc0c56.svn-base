#zoomBar_contenedor {
    bottom:88px;
    height:225px;
    left:3%;
    position:absolute;
    width:48px;
    z-index:5000;

}

    #zoomBar_zonaMover
    {
        posision:relative;
        float:left;
        left:0px;
        top:0px;
        width:48px;
        height:48px;
    }
            /*BOTONES DE MOVIMIENTO*/
                #zoomBar_zonaMover_up
                {
                    position:absolute;
                    left:16px;
                    top:0px;
                    height:16px;
                    width:16px;
                    background-image:url(images/ZoomBar_botonesMovimiento.png);
                    background-position:left top;
                    cursor:pointer;
                }
                        #zoomBar_zonaMover_up:hover
                        {
                            position:absolute;
                            left:16px;
                            top:0px;
                            height:16px;
                            width:16px;
                            background-image:url(images/ZoomBar_botonesMovimiento.png);
                            background-position:right top;
                            cursor:pointer;
                        }
