    #zoomBar_zonaSlider
    {
        posision:relative;
        float:left;
        left:0px;
        top:0px;
        width:48px;
        height:197px;
    }


            #zoomBar_zonaSlider_zoomIn
            {
                position:relative;
                float:left;
                left:0px;
                top:5px;
                height:32px;
                width:100%;
                background-image:url(images/ZoomBar_zoomIn.png);
                background-position:center center;
                background-repeat:no-repeat;
                cursor:pointer;
            }
                    #zoomBar_zonaSlider_zoomIn:hover
                    {
                        position:relative;
                        float:left;
                        left:0px;
                        top:5px;
                        height:32px;
                        width:100%;
                        background-image:url(images/ZoomBar_zoomIn_hover.png);
                        background-position:center center;
                        background-repeat:no-repeat;
                        cursor:pointer;
                    }


            #zoomBar_zonaSlider_slider
            {
                position:relative;
                float:left;
                left:16px;
                top:5px;
                height:124px;
                width:16px;
                background-image:url(images/ZoomBar_slider.png);
                cursor:pointer;
            }
                .knob
                {
                    width: 100%;
                    height: 16px;
                    cursor:pointer;
                    background-image:url(images/ZoomBar_tirador.png);
                }
            #zoomBar_zonaSlider_zoomOut
            {
                position:relative;
                float:left;
                left:0px;
                top:5px;
                height:32px;
                width:100%;
                background-image:url(images/ZoomBar_zoomOut.png);
                background-position:center center;
                background-repeat:no-repeat;
                cursor:pointer;
            }
                    #zoomBar_zonaSlider_zoomOut:hover
                    {
                        position:relative;
                        float:left;
                        left:0px;
                        top:5px;
                        height:32px;
                        width:100%;
                        background-image:url(images/ZoomBar_zoomOut_hover.png);
                        background-position:center center;
                        background-repeat:no-repeat;
                        cursor:pointer;
                    }

