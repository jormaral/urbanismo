<!DOCTYPE composition PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:s="http://jboss.com/products/seam/taglib"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ice="http://www.icesoft.com/icefaces/component"
	template="../layout/template.xhtml">


	<ui:define name="breadcrumb">

		<!--  Se define el breadcrumb -->

		<ice:panelGroup styleClass="breadcrumbTexto">
			<s:link style="color:blue;" value="#{messages.menu_inicio}"
				view="/home.xhtml" id="HomeRutaMenu" propagation="none">
			</s:link>


			<ice:outputLabel styleClass="simboloMayor" value=" > " />

			<s:link style="color:blue;" value="#{messages.menu_informacionfip}"
				view="/informacionfip/InformacionFipNavegacion.xhtml"
				id="InformacionFipRutaMenu" propagation="none">

			</s:link>

			<ice:outputLabel styleClass="simboloMayor" value=" > " />

			<s:link style="color:blue;" value="#{messages.menu_verplanbase}"
				view="/informacionfip/PlanBaseNavegacion.xhtml"
				id="PlanBaseRutaMenu" propagation="none">


			</s:link>




		</ice:panelGroup>



	</ui:define>
	<ui:define name="body">

		<ice:panelGroup styleClass="dr-pnl-sinmargen">

			<!--  Se define el cuerpo de la pagina -->

			<ice:panelGroup columns="4" styleClass="centerHome">

				<s:link
					view="/informacionfip/planbase/entidades/VerEntidadPlanBase.xhtml"
					id="VerEntidadPlanBaseNavegacionMenu" propagation="none">

					<h:graphicImage
						value="/img/editorfip/#{localeSelector.language}/navegacion/04_01_01_CONSULTA_ENTIDADES.png"
						alt="#{messages.menu_consulta_entidades}"
						title="#{messages.menu_consulta_entidades}" styleClass="iconoNav" />

				</s:link>

				<s:link
					view="/informacionfip/planbase/determinaciones/VerPlanBase.xhtml"
					id="VerPlanBaseNavegacionMenu" propagation="none">
					<h:graphicImage
						value="/img/editorfip/#{localeSelector.language}/navegacion/04_01_02_CONSULTA_DETERMINACIONES.png"
						alt="#{messages.menu_consulta_determinaciones}"
						title="#{messages.consulta_determinaciones}" styleClass="iconoNav" />
				</s:link>

				<s:link view="/informacionfip/planbase/unidades/VerUnidad.xhtml"
					id="VerUnidadPlanBaseNavegacionMenu" propagation="none">
					<h:graphicImage
						value="/img/editorfip/#{localeSelector.language}/navegacion/04_01_03_CONSULTA_UNIDADES.png"
						alt="#{messages.menu_consulta_unidades}"
						title="#{messages.menu_consulta_unidades}" styleClass="iconoNav" />
				</s:link>


				<s:link view="/informacionfip/InformacionFipNavegacion.xhtml"
					id="VolverNavegacionMenu" propagation="none">
					<h:graphicImage
						value="/img/editorfip/#{localeSelector.language}/navegacion/00_VOLVER.png"
						alt="#{messages.menu_volver}" title="#{messages.menu_volver}"
						styleClass="iconoNav" />
				</s:link>

			</ice:panelGroup>

		</ice:panelGroup>


	</ui:define>

</ui:composition>
