<div class="toolbarTabs">
	<ul id="panelTramitesTabs" class="tab-menu">
		<li id='tabDatosGeneralesTramitesPublicaciones'><a id="DatosGeneralesTramitesPublicaciones" href="Pages/VisorPLANES/Tramites/DatosGenerarlesTramitesData.html" title="Publicaciones">Publicaciones</a></li>
		<li id='tabDatosGeneralesTramitesTexto' class="selected"><a id="DatosGeneralesTramitesTexto" href="Pages/VisorPLANES/Tramites/DatosGenerarlesTramitesData.html" title="Texto">Texto</a></li>
		<li id='tabDatosGeneralesTramitesComentario'><a id="DatosGeneralesTramitesComentario" href="Pages/VisorPLANES/Tramites/DatosGenerarlesTramitesData.html" title="Comentario">Comentario</a></li>				
	</ul>
	<div class="clear"></div>
</div>

<script type="text/javascript">

	//MochaUI.initializeTabs('panelTramitesTabs','main-panel');	 
	
	$('DatosGeneralesTramitesTexto').addEvent('click', function(e){
		new Event(e).stop();

		if ($('DatosGeneralesTramitesTexto').getStyle('opacity') != 0.5){
			$('panelDatosGeneralesTramitesTexto').setStyle('display', 'block'); //setOpacity(100);
			$('panelDatosGeneralesTramitesComentario').setStyle('display', 'none'); //setOpacity(0);
			$('panelDatosGeneralesTramitesPublicaciones').setStyle('display', 'none'); //setOpacity(0);
			
			$('tabDatosGeneralesTramitesTexto').set('class','selected');
			$('tabDatosGeneralesTramitesComentario').set('class','');
			$('tabDatosGeneralesTramitesPublicaciones').set('class','');
		}		
    });

	$('DatosGeneralesTramitesComentario').addEvent('click', function(e){
		new Event(e).stop();

		if ($('DatosGeneralesTramitesComentario').getStyle('opacity') != 0.5){
			$('panelDatosGeneralesTramitesTexto').setStyle('display', 'none');
			$('panelDatosGeneralesTramitesComentario').setStyle('display', 'block');
			$('panelDatosGeneralesTramitesPublicaciones').setStyle('display', 'none');
			
			$('tabDatosGeneralesTramitesTexto').set('class','');
			$('tabDatosGeneralesTramitesComentario').set('class','selected');
			$('tabDatosGeneralesTramitesPublicaciones').set('class','');
		}		
    });

	$('DatosGeneralesTramitesPublicaciones').addEvent('click', function(e){
		new Event(e).stop();

		if ($('DatosGeneralesTramitesPublicaciones').getStyle('opacity') != 0.5){
			$('panelDatosGeneralesTramitesTexto').setStyle('display', 'none');
			$('panelDatosGeneralesTramitesComentario').setStyle('display', 'none');
			$('panelDatosGeneralesTramitesPublicaciones').setStyle('display', 'block');
			
			$('tabDatosGeneralesTramitesTexto').set('class','');
			$('tabDatosGeneralesTramitesComentario').set('class','');
			$('tabDatosGeneralesTramitesPublicaciones').set('class','selected');
		}		
    });

</script>