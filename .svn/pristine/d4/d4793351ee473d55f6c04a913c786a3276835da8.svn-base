<!DOCTYPE composition PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
                             "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">


<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:s="http://jboss.com/products/seam/taglib"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:h="http://java.sun.com/jsf/html"
		xmlns:ice="http://www.icesoft.com/icefaces/component"  
                template="../layout/template.xhtml">
                       
<ui:define name="body">
    
    <ice:form id="determinacionSearch" styleClass="edit">
      <ice:panelGroup  id="searchGroup" styleClass="formBorderHighlight">
          <table width="100%" border="0" cellspacing="0" cellpadding="0">
              <tr>
                  <td class="iceDatTblColHdr2">
                    <ice:outputText id="listDeterminacionId" value="Determinacion search filter"/>
                 </td>
              </tr>
         </table>	
         <ice:panelGroup id="listPanelGroupDeterminacionId" styleClass="edit">
		
            <s:decorate id="apartadodecId" template="../layout/display.xhtml">
                <ui:define name="label">Apartado</ui:define>
                <ice:inputText id="apartado" 
                          value="#{determinacionList.determinacion.apartado}"
				  partialSubmit="true"/>
            </s:decorate>

            <s:decorate id="codigodecId" template="../layout/display.xhtml">
                <ui:define name="label">Codigo</ui:define>
                <ice:inputText id="codigo" 
                          value="#{determinacionList.determinacion.codigo}"
				  partialSubmit="true"/>
            </s:decorate>

            <s:decorate id="etiquetadecId" template="../layout/display.xhtml">
                <ui:define name="label">Etiqueta</ui:define>
                <ice:inputText id="etiqueta" 
                          value="#{determinacionList.determinacion.etiqueta}"
				  partialSubmit="true"/>
            </s:decorate>

            <s:decorate id="nombredecId" template="../layout/display.xhtml">
                <ui:define name="label">Nombre</ui:define>
                <ice:inputText id="nombre" 
                          value="#{determinacionList.determinacion.nombre}"
				  partialSubmit="true"/>
            </s:decorate>

            <s:decorate id="textodecId" template="../layout/display.xhtml">
                <ui:define name="label">Texto</ui:define>
                <ice:inputText id="texto" 
                          value="#{determinacionList.determinacion.texto}"
				  partialSubmit="true"/>
            </s:decorate>

            <s:decorate template="../layout/display.xhtml">
                <ui:define name="label">Match</ui:define>
                <ice:selectOneRadio id="logic" value="#{determinacionList.restrictionLogicOperator}" partialSubmit="true">
                    <f:selectItem itemLabel="All" itemValue="and"/>
                    <f:selectItem itemLabel="Any" itemValue="or"/>
                </ice:selectOneRadio>
            </s:decorate>
          
   
	  </ice:panelGroup>
  
        
        <div class="actionButtons">
            <ice:commandButton id="search" value="Search" action="/DeterminacionList.xhtml"/>
        </div>
      </ice:panelGroup> 
    </ice:form>
    
    <ice:panelGroup styleClass="formBorderHighlight">

    <h3>determinacion  search results</h3>

    <div class="searchResults" id="listdeterminacionResults">
    <ice:outputText value="The determinacion search returned no results." 
               rendered="#{empty determinacionList.resultList}"/>
               
    <ice:dataTable id="determinacionList" 
                  var="determinacion"
                value="#{determinacionList.resultList}" 
            resizable="true"
	columnClasses="allCols"
             rendered="#{not empty determinacionList.resultList}">
        <ice:column id="listidenId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Iden"/>
                    <ui:param name="propertyPath" value="determinacion.iden"/>
                </ui:include>
            </f:facet>
            #{determinacion.iden}&amp;nbsp;
        </ice:column>
        <ice:column id="listapartadoId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Apartado"/>
                    <ui:param name="propertyPath" value="determinacion.apartado"/>
                </ui:include>
            </f:facet>
            #{determinacion.apartado}&amp;nbsp;
        </ice:column>
        <ice:column id="listbsuspendidaId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Bsuspendida"/>
                    <ui:param name="propertyPath" value="determinacion.bsuspendida"/>
                </ui:include>
            </f:facet>
            #{determinacion.bsuspendida}&amp;nbsp;
        </ice:column>
        <ice:column id="listcodigoId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Codigo"/>
                    <ui:param name="propertyPath" value="determinacion.codigo"/>
                </ui:include>
            </f:facet>
            #{determinacion.codigo}&amp;nbsp;
        </ice:column>
        <ice:column id="listColumndeterminacionByIddeterminacionbaseId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Determinacion by iddeterminacionbase iden"/>
                    <ui:param name="propertyPath" value="determinacion.determinacionByIddeterminacionbase.iden"/>
                </ui:include>
            </f:facet>
            #{determinacion.determinacionByIddeterminacionbase.iden}
        </ice:column>
        <ice:column id="listColumndeterminacionByIddeterminacionoriginalId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Determinacion by iddeterminacionoriginal iden"/>
                    <ui:param name="propertyPath" value="determinacion.determinacionByIddeterminacionoriginal.iden"/>
                </ui:include>
            </f:facet>
            #{determinacion.determinacionByIddeterminacionoriginal.iden}
        </ice:column>
        <ice:column id="listColumndeterminacionByIdpadreId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Determinacion by idpadre iden"/>
                    <ui:param name="propertyPath" value="determinacion.determinacionByIdpadre.iden"/>
                </ui:include>
            </f:facet>
            #{determinacion.determinacionByIdpadre.iden}
        </ice:column>
        <ice:column id="listetiquetaId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Etiqueta"/>
                    <ui:param name="propertyPath" value="determinacion.etiqueta"/>
                </ui:include>
            </f:facet>
            #{determinacion.etiqueta}&amp;nbsp;
        </ice:column>
        <ice:column id="listidcaracterId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Idcaracter"/>
                    <ui:param name="propertyPath" value="determinacion.idcaracter"/>
                </ui:include>
            </f:facet>
            #{determinacion.idcaracter}&amp;nbsp;
        </ice:column>
        <ice:column id="listnombreId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Nombre"/>
                    <ui:param name="propertyPath" value="determinacion.nombre"/>
                </ui:include>
            </f:facet>
            #{determinacion.nombre}&amp;nbsp;
        </ice:column>
        <ice:column id="listordenId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Orden"/>
                    <ui:param name="propertyPath" value="determinacion.orden"/>
                </ui:include>
            </f:facet>
            #{determinacion.orden}&amp;nbsp;
        </ice:column>
        <ice:column id="listtextoId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Texto"/>
                    <ui:param name="propertyPath" value="determinacion.texto"/>
                </ui:include>
            </f:facet>
            #{determinacion.texto}&amp;nbsp;
        </ice:column>
        <ice:column id="listColumntramiteId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{determinacionList}"/>
                    <ui:param name="propertyLabel" value="Tramite iden"/>
                    <ui:param name="propertyPath" value="determinacion.tramite.iden"/>
                </ui:include>
            </f:facet>
            #{determinacion.tramite.iden}
        </ice:column>
        <ice:column id="listColumnDeterminacionId">
            <f:facet name="header">Action</f:facet>
            <s:link view="/seam/#{empty from ? 'Determinacion' : from}.xhtml" 
                   value="#{empty from ? 'View' : 'Select'}" 
                      id="determinacion">
                <f:param name="determinacionIden" 
                        value="#{determinacion.iden}"/>
            </s:link>
			#{' '}
            <s:link view="/seam/DeterminacionEdit.xhtml"
                   value="Edit" 
                      id="determinacionEdit"
                      rendered="#{empty from}">
                <f:param name="determinacionIden" 
                        value="#{determinacion.iden}"/>
            </s:link>
        </ice:column>
    </ice:dataTable>

    </div>
</ice:panelGroup>

    <div class="tableControl">
      
        <s:link view="/seam/DeterminacionList.xhtml" 
            rendered="#{determinacionList.previousExists}" 
               value="#{messages.left}#{messages.left} First Page"
                  id="firstPagedeterminacionListId">
          <f:param name="firstResult" value="0"/>
        </s:link>
        
        <s:link view="/seam/DeterminacionList.xhtml" 
            rendered="#{determinacionList.previousExists}" 
               value="#{messages.left} Previous Page"
                  id="previousPagedeterminacionListId">
            <f:param name="firstResult" 
                    value="#{determinacionList.previousFirstResult}"/>
        </s:link>
        
        <s:link view="/seam/DeterminacionList.xhtml" 
            rendered="#{determinacionList.nextExists}" 
               value="Next Page #{messages.right}"
                  id="nextPagedeterminacionListId">
            <f:param name="firstResult" 
                    value="#{determinacionList.nextFirstResult}"/>
        </s:link>
        
        <s:link view="/seam/DeterminacionList.xhtml" 
            rendered="#{determinacionList.nextExists}" 
               value="Last Page #{messages.right}#{messages.right}"
                  id="lastPagedeterminacionListId">
            <f:param name="firstResult" 
                    value="#{determinacionList.lastFirstResult}"/>
        </s:link>
        
    </div>
    
    <s:div styleClass="actionButtons" rendered="#{empty from}">
        <s:button view="/seam/DeterminacionEdit.xhtml"
                    id="create" 
                 value="Create determinacion">
            <f:param name="determinacionIden"/>
        </s:button>
    </s:div>
    
</ui:define>

</ui:composition>

