package es.mitc.redes.urbanismoenred.servicios.seguridad;

/**
 * Clase que define los roles que pueden ser asignados a los usuarios.
 * 
 * @author Arnaiz Consultores
 *
 */
public class Rol {
	private String nombre;
	private String codigo;
	
	public Rol(String nombre, String codigo) {
		this.nombre = nombre;
		this.codigo = codigo;
	}
	
	public String getNombre() {
		return nombre;
	}
	public String getCodigo() {
		return codigo;
	}
}
