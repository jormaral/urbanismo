package test;

import org.jboss.seam.annotations.Name;


@Name("openLayersBean")
public class OpenLayersBean implements OpenLayers{
	
	private boolean showPanZoomControl;
	private boolean addMetaCartaLayer = true;
	private boolean addNasaLayer;
	private Double lon = new Double(-114);
	private Double lat = new Double(51.1);
	private Integer zoomLevel = 5;

	public Integer getZoomLevel() {
		return zoomLevel;
	}

	public void setZoomLevel(Integer zoomLevel) {
		this.zoomLevel = zoomLevel;
	}

	public Double getLon() {
		return lon;
	}

	public void setLon(Double lon) {
		this.lon = lon;
	}

	public Double getLat() {
		return lat;
	}

	public void setLat(Double lat) {
		this.lat = lat;
	}

	public boolean isAddNasaLayer() {
		return addNasaLayer;
	}

	public void setAddNasaLayer(boolean addNasaLayer) {
		this.addNasaLayer = addNasaLayer;
	}

	public boolean isAddMetaCartaLayer() {
		return addMetaCartaLayer;
	}

	public void setAddMetaCartaLayer(boolean addMetaCartaLayer) {
		this.addMetaCartaLayer = addMetaCartaLayer;
	}

	public boolean isShowPanZoomControl() {
		return showPanZoomControl;
	}

	public void setShowPanZoomControl(boolean showPanZoomControl) {
		this.showPanZoomControl = showPanZoomControl;
	}

	
	

}
