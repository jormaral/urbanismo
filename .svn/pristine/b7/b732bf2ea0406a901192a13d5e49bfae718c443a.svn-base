package com.mitc.redes.editorfip.entidades.interfazgrafico;

public class RegulacionEspecificaDTO {
	private int idRelacion=0;
	private int orden=0;
	private String nombre;
	private String texto;
	private int idRegulacionEspPadre=0;
	private String nombreRegulacionEspPadre;
	
	// CONSTRUCTORES
	public RegulacionEspecificaDTO() {
		super();
		// TODO Auto-generated constructor stub
	}


	public RegulacionEspecificaDTO(int idRelacion, int orden, String nombre,
			String texto, int idRegulacionEspPadre,
			String nombreRegulacionEspPadre) {
		super();
		this.idRelacion = idRelacion;
		this.orden = orden;
		this.nombre = nombre;
		this.texto = texto;
		this.idRegulacionEspPadre = idRegulacionEspPadre;
		this.nombreRegulacionEspPadre = nombreRegulacionEspPadre;
	}

	
	// METODOS

	public int getIdRelacion() {
		return idRelacion;
	}


	public void setIdRelacion(int idRelacion) {
		this.idRelacion = idRelacion;
	}


	public int getOrden() {
		return orden;
	}


	public void setOrden(int orden) {
		this.orden = orden;
	}


	public String getNombre() {
		return nombre;
	}


	public void setNombre(String nombre) {
		this.nombre = nombre;
	}


	public String getTexto() {
		return texto;
	}


	public void setTexto(String texto) {
		this.texto = texto;
	}


	public int getIdRegulacionEspPadre() {
		return idRegulacionEspPadre;
	}


	public void setIdRegulacionEspPadre(int idRegulacionEspPadre) {
		this.idRegulacionEspPadre = idRegulacionEspPadre;
	}


	public String getNombreRegulacionEspPadre() {
		return nombreRegulacionEspPadre;
	}


	public void setNombreRegulacionEspPadre(String nombreRegulacionEspPadre) {
		this.nombreRegulacionEspPadre = nombreRegulacionEspPadre;
	}
	
	
}
