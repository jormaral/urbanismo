<!DOCTYPE composition PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
                             "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">


<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:s="http://jboss.com/products/seam/taglib"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:h="http://java.sun.com/jsf/html"
		xmlns:ice="http://www.icesoft.com/icefaces/component"  
                template="../layout/template.xhtml">
                       
<ui:define name="body">
    
    <ice:form id="documentodeterminacionSearch" styleClass="edit">
      <ice:panelGroup  id="searchGroup" styleClass="formBorderHighlight">
          <table width="100%" border="0" cellspacing="0" cellpadding="0">
              <tr>
                  <td class="iceDatTblColHdr2">
                    <ice:outputText id="listDocumentodeterminacionId" value="Documentodeterminacion search filter"/>
                 </td>
              </tr>
         </table>	
         <ice:panelGroup id="listPanelGroupDocumentodeterminacionId" styleClass="edit">
		
            <s:decorate template="../layout/display.xhtml">
                <ui:define name="label">Match</ui:define>
                <ice:selectOneRadio id="logic" value="#{documentodeterminacionList.restrictionLogicOperator}" partialSubmit="true">
                    <f:selectItem itemLabel="All" itemValue="and"/>
                    <f:selectItem itemLabel="Any" itemValue="or"/>
                </ice:selectOneRadio>
            </s:decorate>
          
   
	  </ice:panelGroup>
  
        
        <div class="actionButtons">
            <ice:commandButton id="search" value="Search" action="/DocumentodeterminacionList.xhtml"/>
        </div>
      </ice:panelGroup> 
    </ice:form>
    
    <ice:panelGroup styleClass="formBorderHighlight">

    <h3>documentodeterminacion  search results</h3>

    <div class="searchResults" id="listdocumentodeterminacionResults">
    <ice:outputText value="The documentodeterminacion search returned no results." 
               rendered="#{empty documentodeterminacionList.resultList}"/>
               
    <ice:dataTable id="documentodeterminacionList" 
                  var="documentodeterminacion"
                value="#{documentodeterminacionList.resultList}" 
            resizable="true"
	columnClasses="allCols"
             rendered="#{not empty documentodeterminacionList.resultList}">
        <ice:column id="listidenId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{documentodeterminacionList}"/>
                    <ui:param name="propertyLabel" value="Iden"/>
                    <ui:param name="propertyPath" value="documentodeterminacion.iden"/>
                </ui:include>
            </f:facet>
            #{documentodeterminacion.iden}&amp;nbsp;
        </ice:column>
        <ice:column id="listColumndeterminacionId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{documentodeterminacionList}"/>
                    <ui:param name="propertyLabel" value="Determinacion iden"/>
                    <ui:param name="propertyPath" value="documentodeterminacion.determinacion.iden"/>
                </ui:include>
            </f:facet>
            #{documentodeterminacion.determinacion.iden}
        </ice:column>
        <ice:column id="listColumndocumentoId">
            <f:facet name="header">
                <ui:include src="../layout/sort.xhtml">
                    <ui:param name="entityList" value="#{documentodeterminacionList}"/>
                    <ui:param name="propertyLabel" value="Documento iden"/>
                    <ui:param name="propertyPath" value="documentodeterminacion.documento.iden"/>
                </ui:include>
            </f:facet>
            #{documentodeterminacion.documento.iden}
        </ice:column>
        <ice:column id="listColumnDocumentodeterminacionId">
            <f:facet name="header">Action</f:facet>
            <s:link view="/seam/#{empty from ? 'Documentodeterminacion' : from}.xhtml" 
                   value="#{empty from ? 'View' : 'Select'}" 
                      id="documentodeterminacion">
                <f:param name="documentodeterminacionIden" 
                        value="#{documentodeterminacion.iden}"/>
            </s:link>
			#{' '}
            <s:link view="/seam/DocumentodeterminacionEdit.xhtml"
                   value="Edit" 
                      id="documentodeterminacionEdit"
                      rendered="#{empty from}">
                <f:param name="documentodeterminacionIden" 
                        value="#{documentodeterminacion.iden}"/>
            </s:link>
        </ice:column>
    </ice:dataTable>

    </div>
</ice:panelGroup>

    <div class="tableControl">
      
        <s:link view="/seam/DocumentodeterminacionList.xhtml" 
            rendered="#{documentodeterminacionList.previousExists}" 
               value="#{messages.left}#{messages.left} First Page"
                  id="firstPagedocumentodeterminacionListId">
          <f:param name="firstResult" value="0"/>
        </s:link>
        
        <s:link view="/seam/DocumentodeterminacionList.xhtml" 
            rendered="#{documentodeterminacionList.previousExists}" 
               value="#{messages.left} Previous Page"
                  id="previousPagedocumentodeterminacionListId">
            <f:param name="firstResult" 
                    value="#{documentodeterminacionList.previousFirstResult}"/>
        </s:link>
        
        <s:link view="/seam/DocumentodeterminacionList.xhtml" 
            rendered="#{documentodeterminacionList.nextExists}" 
               value="Next Page #{messages.right}"
                  id="nextPagedocumentodeterminacionListId">
            <f:param name="firstResult" 
                    value="#{documentodeterminacionList.nextFirstResult}"/>
        </s:link>
        
        <s:link view="/seam/DocumentodeterminacionList.xhtml" 
            rendered="#{documentodeterminacionList.nextExists}" 
               value="Last Page #{messages.right}#{messages.right}"
                  id="lastPagedocumentodeterminacionListId">
            <f:param name="firstResult" 
                    value="#{documentodeterminacionList.lastFirstResult}"/>
        </s:link>
        
    </div>
    
    <s:div styleClass="actionButtons" rendered="#{empty from}">
        <s:button view="/seam/DocumentodeterminacionEdit.xhtml"
                    id="create" 
                 value="Create documentodeterminacion">
            <f:param name="documentodeterminacionIden"/>
        </s:button>
    </s:div>
    
</ui:define>

</ui:composition>

