<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <servlet>
  <servlet-name>RestMethod</servlet-name>
  <servlet-class>es.mitc.redes.urbanismoenred.rest.RestMethod</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>FichaUrbanistica</servlet-name>
  <servlet-class>es.mitc.redes.urbanismoenred.ficha.FichaUrbanistica</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>FichaGenerica</servlet-name>
  <servlet-class>es.mitc.redes.urbanismoenred.ficha.FichaGenerica</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>FichaSuperficies</servlet-name>
  <servlet-class>es.mitc.redes.urbanismoenred.ficha.FichaSuperficies</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>FichaConfigurada</servlet-name>
  <servlet-class>es.mitc.redes.urbanismoenred.ficha.FichaConfigurada</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>GetDocumento</servlet-name>
  <servlet-class>es.mitc.redes.urbanismoenred.serviciosweb.GetDocumento</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>mapfish.print</servlet-name>
  <servlet-class>es.mitc.redes.urbanismoenred.ficha.MapPrinterServlet</servlet-class>
  <init-param>
   <param-name>config</param-name>
   <param-value>/urbr/app/conf/config.yaml</param-value>
  </init-param>
 </servlet>
 <servlet>
  <servlet-name>urbrWS</servlet-name>
  <servlet-class>es.mitc.redes.urbanismoenred.serviciosweb.urbrWS</servlet-class>
  <load-on-startup>1</load-on-startup>
 </servlet>
 <servlet>
  <display-name>Gestor de FIPs</display-name>
  <servlet-name>GestorFips</servlet-name>
  <servlet-class>es.mitc.redes.urbanismoenred.serviciosweb.GestorFips</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>RestMethod</servlet-name>
  <url-pattern>/RestMethod</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>GetDocumento</servlet-name>
  <url-pattern>/GetDocumento</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>FichaUrbanistica</servlet-name>
  <url-pattern>/FichaUrbanistica</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>FichaConfigurada</servlet-name>
  <url-pattern>/FichaConfigurada</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>FichaSuperficies</servlet-name>
  <url-pattern>/FichaSuperficies</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>FichaGenerica</servlet-name>
  <url-pattern>/FichaGenerica</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>mapfish.print</servlet-name>
  <url-pattern>/Print/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>urbrWS</servlet-name>
  <url-pattern>/urbrWS</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>GestorFips</servlet-name>
  <url-pattern>/FIPs/*</url-pattern>
 </servlet-mapping>
 <session-config>
  <session-timeout>30</session-timeout>
 </session-config>
 <welcome-file-list>
  <welcome-file>index.jsp</welcome-file>
 </welcome-file-list>
</web-app>
