.mooTree_node {
	font-family: Verdana, Arial, Helvetica;
	font-size: 10px;
	white-space: nowrap;
        z-index:100;
        /*width:400px;*/
        background-color: transparent;
}

.mooTree_text {
	padding-top: 4px;
	height: 15px;
	cursor: pointer;
        z-index:100;
        /*width:300px;*/
        background-color: transparent;
        overflow:visible;
        color:#333333;
}

.mooTree_img {
	float: left;
	width: 16px;
	height: 16px;
	overflow: hidden;
        z-index:100;
        background-color: transparent;
}

.mooTree_selected	 {
	background-color: #dddddd;
	font-weight: bold;
        /*width:300px;*/
        z-index:100;
}


.cloneElementClass
{
    z-index:80;
}